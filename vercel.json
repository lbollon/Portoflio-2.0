{
  "builds": [
    { "src": "portfolio.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "scripts.js", "use": "@vercel/static" },
    { "src": "images/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/portfolio.html" },
    { "src": "/(.*)", "dest": "/portfolio.html" }
  ]
}
